{
  "name": "forumpay/forumpay-prestashop",
  "description": "ForumPay Crypto Payments for PrestaShop",
  "type": "prestashop-module",
  "version": "2.0.3",
  "license": "OSL-3.0",
  "require": {
    "php": ">=7.4",
    "forumpay/payment-gateway-php-client": "^v1.1.3",
    "psr/log":"^1.1.4"
  },
  "require-dev": {
    "prestashop/php-dev-tools": "^4"
  },
  "autoload": {
    "psr-4": {
      "ForumPay\\PaymentGateway\\PrestaShopModule\\": "src/"
    }
  },
  "minimum-stability": "dev",
  "config": {
    "preferred-install": "dist",
    "prepend-autoloader": false,
    "allow-plugins": {
      "composer/installers": true
    }
  }
}
